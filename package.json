{
  "name": "react-redux-isomorphic",
  "version": "0.3.1",
  "description": "Set of utilities for creating isomorphic applications using react-redux",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "scripts": {
    "build": "yarn build:lib && yarn build:es",
    "build:lib": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\"",
    "lint": "eslint src --ext .js,.jsx",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "cross-env BABEL_ENV=test jest",
    "test:unit:watch": "cross-env BABEL_ENV=test jest --watchAll",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtaits/react-redux-isomorphic.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "ssr",
    "isomorphic",
    "react-redux-isomorphic",
    "decorator",
    "context"
  ],
  "author": "Vadim Taits",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtaits/react-redux-isomorphic/issues"
  },
  "homepage": "https://github.com/vtaits/react-redux-isomorphic#readme",
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-redux": "^4.3.0 || ^5.0.0 || ^6.0.0",
    "redux": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "check-prop-types": "^1.1.1",
    "cross-env": "^5.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flux-standard-action": "^2.0.0",
    "jest-cli": "^23.6.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-test-renderer": "^16.6.3",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "setupFiles": [
      "./setup-jest.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "es6-error": "^4.1.1",
    "invariant": "^2.2.2"
  }
}
